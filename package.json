{
  "name": "bs-msw",
  "version": "0.1.0",
  "description": "Mock Service Worker (MSW) bindings for BuckleScript in Reason",
  "repository": "https://github.com/jihchi/bs-msw.git",
  "homepage": "https://github.com/jihchi/bs-msw",
  "bugs": {
    "url": "https://github.com/jihchi/bs-msw/issues"
  },
  "scripts": {
    "build": "bsb -make-world",
    "clean": "bsb -clean-world",
    "format": "find ./src ./__tests__ -regex '.*\\.rei\\{0,1\\}' -exec bsrefmt --in-place '{}' \\;",
    "prepublishOnly": "npm run build",
    "pretest": "npm run build",
    "start": "npm run build -- -w",
    "test": "jest"
  },
  "keywords": [
    "BuckleScript",
    "ReasonML",
    "bindings",
    "mock",
    "mocking",
    "msw"
  ],
  "author": "Jihchi Lee <achi@987.tw>",
  "license": "MIT",
  "files": [
    "/src/**/*.re",
    "bsconfig.json"
  ],
  "dependencies": {},
  "devDependencies": {
    "@glennsl/bs-jest": "^0.5.1",
    "bs-fetch": "^0.5.2",
    "bs-platform": "^7.2.2",
    "msw": "^0.17.0",
    "node-fetch": "^2.6.0"
  },
  "peerDependencies": {
    "msw": "^0.17.0"
  }
}
